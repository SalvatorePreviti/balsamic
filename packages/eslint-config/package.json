  "peerDependencies": {
    "@balsamic/tsn": ">=0.0.6",
    "@types/node": ">=20.8.0",
    "@typescript-eslint/eslint-plugin": ">=6.7.3",
    "@typescript-eslint/parser": ">=6.7.3",
    "@swc/core": ">=1.3.82",
    "eslint": ">=8.50.0",
    "eslint-plugin-node": ">=11.1.0",
    "eslint-plugin-json": ">=3.1.0",
    "eslint-plugin-import": "^2.28.1",
    "prettier": "^3.0.3",
    "ts-node": ">=10.9.1",
    "tsconfig-paths": ">=4.2.0",
    "typescript": ">=5.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.0"
  }
