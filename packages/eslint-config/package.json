    "@typescript-eslint/eslint-plugin": ">=7.3.1",
    "@typescript-eslint/parser": ">=7.3.1",
    "@swc/core": "^1.4.8",
    "eslint": ">=8.57.0",
    "eslint-plugin-node": ">=11.1.0",
    "eslint-plugin-json": ">=3.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.5",
    "tsconfig-paths": ">=4.2.0",
    "typescript": ">=5.4.3"
