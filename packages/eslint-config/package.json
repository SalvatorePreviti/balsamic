{
  "name": "@balsamic/eslint-config",
  "version": "0.6.7",
  "description": "balsamic eslint configuration",
  "keywords": [
    "balsamic",
    "eslint",
    "eslint-config",
    "prettierrc"
  ],
  "private": false,
  "license": "MIT",
  "author": "Salvatore Previti",
  "homepage": "https://github.com/SalvatorePreviti/balsamic#readme",
  "bugs": {
    "url": "https://github.com/SalvatorePreviti/balsamic/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalvatorePreviti/balsamic.git"
  },
  "eslintConfig": {
    "$schema": "http://json.schemastore.org/prettierrc",
    "extends": "@balsamic"
  },
  "bin": {
    "balsamic-eslint": "scripts/main.js"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "index.js",
  "dependencies": {
