{
  "name": "@balsamic/estest",
  "version": "0.0.1",
  "description": "transpiles, bundles and run ts and es6 modules on NodeJS using esbuild",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalvatorePreviti/balsamic.git"
  },
  "keywords": [
    "esbuild",
    "esrun",
    "estest",
    "es-run",
    "es-test"
  ],
  "author": "Salvatore Previti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SalvatorePreviti/balsamic/issues"
  },
  "homepage": "https://github.com/SalvatorePreviti/balsamic/tree/main/packages/estest#readme",
  "bin": {
    "estest": "estest.js"
  },
  "files": [
    "**/*.js",
    "**/*.d.ts"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ]
  },
  "engines": {
    "node": ">=14.15.5"
  },
  "dependencies": {
