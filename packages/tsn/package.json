{
  "name": "@balsamic/tsn",
  "version": "0.0.7",
  "description": "typescript tooling",
  "type": "commonjs",
  "sideEffects": true,
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalvatorePreviti/balsamic.git",
    "directory": "packages/tsn"
  },
  "keywords": [
    "balsamic",
    "tsn"
  ],
  "author": "Salvatore Previti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SalvatorePreviti/balsamic/issues"
  },
  "homepage": "https://github.com/SalvatorePreviti/balsamic/tree/main/packages/dev#readme",
  "bin": {
    "tsn": "./tsn",
    "tscheck": "./tscheck/tscheck.js"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./index": {
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./index.js": {
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./repl.js": "./repl.js",
    "./git-subtree": "./git-subtree.js",
    "./git-subtree.js": "./git-subtree.js",
    "./index.mjs": "./index.mjs",
    "./package.json": "./package.json",
    "./tsconfig.json": "./tsconfig.json",
    "./tsconfig-loose.json": "./tsconfig-loose.json",
    "./tscheck": "./tscheck/index.js",
    "./tscheck.js": "./tscheck/index.js"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "app-root-path": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0"
  },
  "peerDependencies": {
    "typescript": ">=5.2.2",
    "ts-node": ">=10.9.1",
    "tsconfig-paths": ">=4.2.0",
    "@swc/core": ">=1.3.82"
  }
}
